{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "cache"
  },
  "exclude": [
    "node_modules",
    "cache",
    "dist",
    "reports"
  ],
  "include": [
    "src/**/*.ts"
  ]
}